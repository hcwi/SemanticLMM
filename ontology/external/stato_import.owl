    <owl:Class rdf:about="http://purl.obolibrary.org/obo/STATO_0000301">
        <rdfs:label xml:lang="en">covariance</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
                <owl:onClass rdf:resource="http://purl.obolibrary.org/obo/STATO_0000230"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
